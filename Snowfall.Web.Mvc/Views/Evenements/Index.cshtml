@model EvenementsIndexViewModel;

@{
    ViewBag.Title = "Événements";
}

<h1>Évènements</h1>

<div class="row">
    <div class="col-lg-3 mb-4">
        <partial name="_Filtres" model="Model.FiltresEvenements" />
    </div>
    <div class="col-lg-9">
        <div class="row">
            @foreach (Evenement evenement in Model.Evenements)
            {
                <partial name="_Evenement" model="evenement"/>
            }
        </div>
    </div>
</div>
